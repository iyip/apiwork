/admin:
  description: Main entry point for administration, includes links to user, permissions, roles and accounts.
  get:
    description: If details parameter is present with value being true, then the json payload will contain all available information about the user. If details parameter is absent or present with value being false, then only the href for user will be returned. Client needs to follow the url to get more information.
    is: [detailParams]
    responses:
      200:
        body:
          application/json:
            example: |
                {
                  "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/",
                  "type": "application/vnd.akamai.answerx.admin-root.v1",
                  "description": "Main entry point for administration.",
                  "accounts": {
                    "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/accounts/",
                    "type": "application/vnd.akamai.answerx.account.v1; type=group",
                    "description": "A collection of account entities in the system."
                  },
                  "roles": {
                    "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/roles/",
                    "type": "application/vnd.akamai.answerx.role.v1; type=group",
                    "description": "A collection of role entities in the system."
                  },
                  "permissions": {
                    "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/permissions/",
                    "type": "application/vnd.akamai.answerx.permission.v1; type=group",
                    "description": "A collection of permission entities in the system."
                  },
                  "user": {
                    "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/user/456b0a5e-a78c-478b-34dd-0828f73e5788",
                    "type": "application/vnd.akamai.answerx.user.v1"
                    }
                  }
                }
  /user/{userId}:
    get:
      description: Get detailed information about a user identified by the user Id, including his roles and permissions and the account he has access to.
      responses:
        200:
          body:
            application/json:
              example: |
                  {
                    "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/user/456b0a5e-a78c-478b-34dd-0828f73e5788",
                    "type": "application/vnd.akamai.answerx.user.v1",
                    "description": "authenticated user who is currently logged into the system.",
                    "id": "456b0a5e-a78c-478b-34dd-0828f73e5788",
                    "username": "ccare",
                    "firstName": "CustomerCare",
                    "lastName": "Support",
                    "description": "Customer Care and Support User",
                    "email": "ccare_user@akamai.com",
                    "status": "Active",
