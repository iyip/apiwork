                    "account": {
                      "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/account/2-5H9G-4",
                      "type": "application/vnd.akamai.answerx.account.v1",
                      "name": "ACME",
                      "id": "2-5H9G-4",
                      "configured": false
                    },
                    "roles": {
                      "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/user/456b0a5e-a78c-478b-34dd-0828f73e5788/roles/",
                      "type": "application/vnd.akamai.answerx.role.v1; type=group",
                      "items": [
                        {
                          "id": "66c4aa56-45cf-40b4-b0c0-9a4501a25aaa",
                          "name": "Role1",
                          "description": "Role1"
                        },
                        {
                          "id": "8904aa56-45cf-40b4-b0c0-9a4501a2502e",
                          "name": "Role2",
                          "description": "Role2"
                        }
                      ]
                    },
                    "permissions": {
                      "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/user/456b0a5e-a78c-478b-34dd-0828f73e5788/permissions/",
                      "type": "application/vnd.akamai.answerx.permission.v1; type=group",
