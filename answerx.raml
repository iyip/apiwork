  /account/{accountId}/:
    get:
      description: Get the details of a specific account, identified by its id. If the configured flag of the account is false, then upon initial access, user must go through the setup process to provide the anycastIP and customerId, before he can access the system. If the configured flag is true, then the account has already been setup (one time process).
      responses:
        200:
          body:
            application/json:
              example: |
                  {
                    "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/account/2-5H9G-4",
                    "type": "application/vnd.akamai.answerx.account.v1",
                    "name": "ACME",
                    "id": "2-5H9G-4",
                    "configured": false
                  }
    patch:
      description: Update a specific account, identified by its id.
      body:
        example: |
          {
            "anycastIP": "1.2.3.4",
            "customerId" : "3456-9087-er45",
            "type": ["typeB", "typeC"]
          }
      responses:
        200:
          body:
            example: |
              {
                "href": "http://open.luna.akamaiapis.net/answerx/v1/admin/account/2-5H9G-4",
                "type": "application/vnd.akamai.answerx.account.v1",
                "name": "ACME",
                "id": "2-5H9G-4",
                "anycastIP": "1.2.3.4",
                "customerId" : "3456-9087-er45",
                "type": ["typeB", "typeC"]
                "configured": true
              }

traits:
  - pageParams:
      queryParameters:
        start:
          type: integer
          description: starting index.
          required: false
          default: 0
          example: 20
        pageSize:
          type: integer
          description: page size, number of items per page.
          required: false
          default: 20
          example: 50
  - detailParams:
      queryParameters:
        detail:
          type: boolean
          description: expand url to show more details.
          required: false
          default: false
          example: true
  - timeParams:
      queryParameters:
        startTime:
          type: number
          description: Start time of report, in the format of YYYY-MM-DDThh:mm:ssZ in UTC time
          required: true
          example: 1437864142000
        endTime:
          type: number
          description: End time of report, in the format of YYYY-MM-DDThh:mm:ssZ in UTC time
          required: true
          example: 1443220943000
        unit:
          type: string
          description: unit of time
          required: true
          enum: [MINUTE, HOUR, DAY, WEEK, MONTH]
          example: MINUTE
        interval:
          type: number
          description: the interval of time for each time bucket in unit
          required: true
          example: 5
  - metricTypeParams:
      queryParameters:
        metricType:
          type: string
          description: The criteria to use to get the metrics data.
          required: true
          enum: [ResponseTimeLevel1, ResponseTimeLevel2, ServiceAvailability, Region, ResponseType, All]
          example: All
