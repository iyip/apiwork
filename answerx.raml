  /{jobId}:
    description: Get a specific job identified by the job Id. Job status can be either COMPLETED, FAILED or PROCESSING. There will be no information available on percentage completion.
    get:
      is: [timeParams]
      responses:
        200:
          body:
            application/json:
              example: |
                {
                  "href": "http://private-e583dd-pulsaranswerx.apiary-mock.com/answerx/v1/jobs/66554433",
                  "type": "application/vnd.akamai.answerx.job.v1",
                  "id": "66554433",
                  "status": "PROCESSING",
                }
/reports:
  description: Main entry point for reports, including links to reports such as Request vs Response Overview, Requests Detail, Responses Detail, Response Time SLA and Service Availability SLA.
  get:
    responses:
      200:
        body:
          application/json:
            example: |
              {
                "href": "http://open.luna.akamaiapis.net/answerx/v1/reports/",
                "type": "application/vnd.akamai.answerx.report-root.v1",
                "description": "Main entry point for all available report types",
                "reqRes": {
                  "href": "http://open.luna.akamaiapis.net/answerx/v1/reports/reqRes",
                  "type": "application/vnd.akamai.answerx.report.v1",
                  "description": "Total DNS requests and responses report, for a given time period."
                },
                "requests": {
                  "href": "http://open.luna.akamaiapis.net/answerx/v1/reports/requests",
                  "type": "application/vnd.akamai.answerx.report.v1",
                  "description": "DNS requests report, break down by region, for a given time period."
                },
                "responses": {
                  "href": "http://open.luna.akamaiapis.net/answerx/v1/reports/responses",
                  "type": "application/vnd.akamai.answerx.report.v1",
                  "description": "DNS responses report, break down by region, or by response type, for a given time period."
                },
                "responseTimeSLA": {
                  "href": "http://open.luna.akamaiapis.net/answerx/v1/reports/responseTimeSLA",
                  "type": "application/vnd.akamai.answerx.report.v1",
                  "description": "Response time SLA report, including SLA level1 and level2, for a given time period."
                },
                "serviceAvailSLA": {
                  "href": "http://open.luna.akamaiapis.net/answerx/v1/reports/serviceAvailSLA",
                  "type": "application/vnd.akamai.answerx.report.v1",
                  "description": "Service availability SLA report, for the given time period."
                }
              }
  /reqRes:
    get:
